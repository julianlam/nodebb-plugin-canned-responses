    "validator": "^12.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-angular": "12.0.1",
    "eslint": "7.21.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "husky": "5.1.3",
    "lint-staged": "10.5.4"
